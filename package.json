{
  "name": "@unstable/addimated",
  "version": "0.0.0-development",
  "description": "An always interruptible, declarative animation library for React",
  "keywords": [
    "Additive",
    "Animated",
    "Animation",
    "React"
  ],
  "homepage": "https://github.com/vincentriemer/addimated",
  "bugs": {
    "url": "https://github.com/vincentriemer/addimated/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vincentriemer/addimated.git"
  },
  "license": "MIT",
  "author": {
    "name": "Vincent Riemer",
    "url": "https://github.com/vincentriemer"
  },
  "files": [
    "lib",
    "src",
    "scripts/unstableWarning.js",
    "scripts/paths.js",
    "LICENSE.md",
    "THIRD-PARTY-LICENSES.txt",
    "index.d.ts"
  ],
  "main": "lib/addimated.cjs.js",
  "module": "lib/addimated.js",
  "scripts": {
    "build": "vincent-scripts build",
    "postinstall": "node scripts/unstableWarning.js",
    "prepublishOnly": "yarn build",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "bezier-easing": "^2.1.0",
    "invariant": "^2.2.4",
    "normalize-css-color": "^1.0.2",
    "warning": "^4.0.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.3.1",
    "@types/react": "^16.7.20",
    "@types/react-native": "^0.57.29",
    "@vincentriemer/scripts": "^1.0.1",
    "flow-bin": "^0.91.0",
    "husky": "^1.3.1",
    "import-sort-cli": "^5.2.0",
    "import-sort-parser-babylon": "^5.2.0",
    "import-sort-style-module": "^5.0.0",
    "lint-staged": "^8.1.0",
    "node": "^11.0.0",
    "prettier": "^1.16.1",
    "react": "16.8.0-alpha.1",
    "semantic-release": "^17.2.3",
    "sort-package-json": "^1.17.1"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.0.0",
    "react": "16.8.0-alpha.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module"
    },
    ".mjs": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "lint-staged": {
    "*.{mjs,js}": [
      "import-sort --write",
      "git add"
    ],
    "*.{mjs,js,json,md}": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "module:modern": "lib/addimated.mjs",
  "release": {
    "branches": [
      "master",
      {
        "name": "develop",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "sideEffects": false,
  "source": "src/index.js"
}
